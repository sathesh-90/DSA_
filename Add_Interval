class Solution(object):
    def insert(self, intervals, newInterval):
        res = []
        
        for interval in intervals:
            # Case 1: No overlap, interval completely before newInterval
            if interval[1] < newInterval[0]:
                res.append(interval)
            # Case 2: No overlap, interval completely after newInterval
            elif interval[0] > newInterval[1]:
                res.append(newInterval)
                newInterval = interval  # treat this as the new one to add later
            # Case 3: Overlap -> merge
            else:
                newInterval[0] = min(newInterval[0], interval[0])
                newInterval[1] = max(newInterval[1], interval[1])
        
        res.append(newInterval)  # add the last interval
        return res
